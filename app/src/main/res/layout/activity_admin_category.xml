<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminCategory"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="5dp">

        <ScrollView
            android:id="@+id/homeFragmentHorizontalScrollView"
            android:layout_width="wrap_content"
            android:layout_marginStart="8sp"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/mobiles"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80dp"
                        android:layout_height="80sp"
                        android:src="@drawable/phone" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/mobiles"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/earbuds"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80dp"
                        android:src="@drawable/earbuds" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/earbuds"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80dp"
                        android:src="@drawable/tv" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/laptop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/leptop"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/laptops"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/headphone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/headphone"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/headphones"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/speaker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/speaker"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/speakers"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/keyword"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/keyboard"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/keywords"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mouse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/mouse"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/mouses"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/camera"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/camero"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/camera"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/smartwatch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/watch"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/smartwatches"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/tablet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_margin="10sp">

                    <ImageView
                        android:layout_width="80sp"
                        android:layout_height="80sp"
                        android:src="@drawable/tablet"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tablets"
                        android:textSize="13sp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:fontFamily="@font/poppinsregular"/>

                </LinearLayout>


            </LinearLayout>

        </ScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- Mobile Company -->
            <HorizontalScrollView
                android:id="@+id/mobileHorizontalScrollView"
                android:layout_width="match_parent"
                android:layout_marginStart="8sp"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <!--            iPhone-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/iphone"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/iphone"
                            android:layout_marginHorizontal="10sp"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/iphone"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                    <!--            Samsung-->
                    <LinearLayout
                        android:id="@+id/samsung"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/samsung"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/samsung"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                    <!--            Vivo-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="10sp"
                        android:id="@+id/vivo"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_gravity="center"
                            android:src="@drawable/vivo"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/vivo"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                    <!--            Oppo-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/oppo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_gravity="center"
                            android:src="@drawable/oppo"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/oppo"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                    <!--            MI-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:id="@+id/mi"
                        android:layout_marginHorizontal="10sp"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_gravity="center"
                            android:src="@drawable/mi"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/redmi"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                    <!--            Realme-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:id="@+id/realme"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="10sp"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/realme"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:text="@string/realme"
                            android:fontFamily="@font/poppinsregular"/>

                    </LinearLayout>

                </LinearLayout>

            </HorizontalScrollView>

            <!-- Earbuds Company -->
            <HorizontalScrollView
                android:id="@+id/earphoneHorizontalScrollView"
                android:layout_width="match_parent"
                android:layout_marginStart="8sp"
                android:layout_marginTop="10sp"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/boatlogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/boat" />


                    </LinearLayout>



                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/realmelogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/realmelogo"
                            android:background="@drawable/round"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/onepluslogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/onepluslogo" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/nothinglogo"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="10sp"
                        android:orientation="vertical">


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/nothing1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/triggerlogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/trigger" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/trukelogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/truke" />


                    </LinearLayout>

                </LinearLayout>

            </HorizontalScrollView>

            <!-- TV Company -->
            <HorizontalScrollView
                android:id="@+id/tvHorizontalScrollView"
                android:layout_width="match_parent"
                android:layout_marginStart="8sp"
                android:layout_marginTop="10sp"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/samsunglogo"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/samsunglogo" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/lglogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/lg"
                            android:background="@drawable/round"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/milogo"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/mi"
                            android:background="@drawable/round"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tcllogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/tcl" />


                    </LinearLayout>


                </LinearLayout>

            </HorizontalScrollView>

            <!-- Laptop Company -->
            <HorizontalScrollView
                android:id="@+id/laptopHorizontalScrollView"
                android:layout_width="match_parent"
                android:layout_marginStart="8sp"
                android:layout_marginTop="10sp"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/hplogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/hp" />


                    </LinearLayout>



                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/delllogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:src="@drawable/dell"
                            android:background="@drawable/round"
                            android:layout_gravity="center"
                            android:layout_height="60dp"/>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/lenovologo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/lenovo" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/acerlogo"
                        android:layout_weight="1"
                        android:layout_marginHorizontal="10sp"
                        android:orientation="vertical">


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/acer" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/asusogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/asus" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/applelogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/applelogo" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/msilogo"
                        android:layout_marginHorizontal="10sp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="61dp"
                            android:layout_height="60dp"
                            android:layout_gravity="center"
                            android:background="@drawable/round"
                            android:src="@drawable/msi" />


                    </LinearLayout>

                </LinearLayout>

            </HorizontalScrollView>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/adminCategoryRecyclerview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
